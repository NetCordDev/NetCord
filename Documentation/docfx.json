{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "NetCord/bin/Release/net6.0/NetCord.dll",
            "NetCord.Services/bin/Release/net6.0/NetCord.Services.dll"
          ]
        }
      ],
      "dest": "docs"
    }
  ],
  "build": {
    "sitemap": {
      "baseUrl": "https://netcord.dev"
    },
    "content": [
      {
        "files": [
          "docs/**.yml",
          "docs/index.md"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "logo.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default",
      "templates/NetCord"
    ],
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "_appDescription": "NetCord â€“ a lightweight and asynchronous Discord API wrapper",
      "_appTitle": "NetCord Documentation",
      "_enableSearch": true,
      "_disableFooter": true,
      "_noindex": true
    },
    "fileMetadata": {
      "_noindex": {
        "docs/**": false
      },
      "_enableSearch": {
        "!docs/**": false
      }
    },
    "xrefService": "https://xref.docs.microsoft.com/query?uid={uid}"
  }
}
